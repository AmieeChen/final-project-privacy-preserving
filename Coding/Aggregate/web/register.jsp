      <div  class="form">
    		<form id="contactform" action="Register" method="post"> 
    			<p class="contact"><label for="name">User Name</label></p> 
    			<input id="name" name="name" placeholder="First and last name"  tabindex="1" type="text" title="UserName (UpperCase, LowerCase, Number/SpecialChar and min 8 Chars)" pattern="(?=^.{8,}$)((?=.*\d)|(?=.*\W+))(?![.\n])(?=.*[A-Z])(?=.*[a-z]).*$" required=""> 
    			 
    			<p class="contact"><label for="email">Email</label></p> 
    			<input id="email" name="email" placeholder="example@domain.com" title="Please enter Valid Email Id" pattern="[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3}$" required="" type="text"> 
                
